sno=int(input('enter student no:'))
name=input('enter student name:')
gender=input('enter gender:')
group=input('enter group:')
address=input('enter address:')
phno=int(input('enter phno:'))
print('sno:',sno)
print('name:',name)
print('gender:',gender)
print('group:',group)
print('address:',address)
print('phno:',phno)

